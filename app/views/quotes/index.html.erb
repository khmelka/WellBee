<h1>WellBee 🐝</h1>
<p id="notice"><%= notice %></p>
<h2>Inspirational Quotes</h2>

<ul class="navigation">
  <li><%= link_to 'Back', user_path(params[:user_id]) %></li>
</ul>

  
<ul class="quotes">
  <% @quotes.each do |quote| %>
   
    <li><mark><%= quote.quote_content %></mark></li>
      
    <li class="author">-<%= quote.quote_author %></li>
    <br>
  <% end %>
</ul>
  

