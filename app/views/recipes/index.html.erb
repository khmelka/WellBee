<h1><%= link_to 'WellBee 🐝', users_path %></h1>
<p id="notice"><%= notice %></p>
<h2>Recipes</h2>

<ul class="navigation" style="margin-top: -6.7rem">
  <li><%= link_to 'Back', user_path(params[:user_id]) %></li>
</ul>

  
<ul>
  <% @recipes.each do |recipe| %>
   
    <li class="flex-container-rec"><%= recipe.recipe_title %></li>
      <br>
    <li class="flex-item-rec"><%= recipe.recipe_content %></li>
    <br>
  <% end %>
</ul>
  

